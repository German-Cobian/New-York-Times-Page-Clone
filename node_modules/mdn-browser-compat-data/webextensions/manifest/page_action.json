{
  "webextensions": {
    "manifest": {
      "page_action": {
        "__compat": {
          "mdn_url": "https://developer.mozilla.org/docs/Mozilla/Add-ons/WebExtensions/manifest.json/page_action",
          "support": {
            "chrome": [
              {
                "version_added": true,
                "notes": "If an extension defines a page action, it is not allowed to define a browser action as well."
              },
              {
                "version_added": "49",
                "notes": "Since Chrome 49, page actions are displayed on the toolbar, rather than in the address bar."
              }
            ],
            "edge": {
              "version_added": "14"
            },
            "firefox": {
              "version_added": "48"
            },
            "firefox_android": {
              "version_added": true
            },
            "opera": {
              "version_added": true,
              "notes": "If an extension defines a page action, it is not allowed to define a browser action as well."
            },
            "safari": {
              "version_added": "14",
              "notes": "If an extension defines a page action, it is not allowed to define a browser action as well."
            }
          }
        },
        "browser_style": {
          "__compat": {
            "support": {
              "chrome": {
                "version_added": false
              },
